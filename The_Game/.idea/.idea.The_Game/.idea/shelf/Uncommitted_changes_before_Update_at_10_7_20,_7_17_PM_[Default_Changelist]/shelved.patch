+using UnityEngine;
+
+public class BaseCharacter
+{
+    //element type
+    private string characterElementType;
+    private string characterDescription;
+    //stats
+    private int Level;
+    private int HP;       //cap at 1000? 
+    private int Attack;   //cap at 200?
+    private int Defense;  //        |
+    private int Speed;    //        |
+    private int Accuracy; //        V
+    
+    //getters and setters for BaseCharacter
+    public string CharacterElementType
+    {
+        get => characterElementType;
+        set => characterElementType = value;
+    }//end of CharacterElementType
+    
+    public string CharacterDescription
+    {
+        get => characterDescription;
+        set => characterDescription = value;
+    }//end of CharacterElementType
+    
+    public int level
+    {
+        get => Level;
+        set => Level = value;
+    }//end of level
+    public int hp
+    {
+        get => HP;
+        set => HP = value;
+    }//end of hp
+
+    public int attack
+    {
+        get => Attack;
+        set => Attack = value;
+    }//end of attack
+    
+    public int defense
+    {
+        get => Defense;
+        set => Defense = value;
+    }//end of defense
+    
+    public int speed
+    {
+        get => Speed;
+        set => Speed = value;
+    }//end of speed
+    
+    public int accuracy
+    {
+        get => Accuracy;
+        set => Accuracy = value;
+    }//end of accuracy
+    
+    
+}//end of class BaseCharacterClass
