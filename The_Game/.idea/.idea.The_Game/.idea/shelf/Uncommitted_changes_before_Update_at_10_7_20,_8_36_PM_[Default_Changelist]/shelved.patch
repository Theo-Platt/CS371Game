+            timer -= Time.deltaTime;
+            if (timer <= 0f) {
+                timer = timerMax;
+                myRenderer.sortingOrder = (int)(sortingOrderBase - transform.position.y - offset);
+                if (runOnlyOnce) {
+                    Destroy(this);
+                }
